<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cszt.test1.mapper.UserMapperTest1">
    <select id="getUser" parameterType="integer" resultType="com.User">
        select u.id,u.name,u.pwd from user u where u.id = #{id}
    </select>
    <insert id="insert" parameterType="com.User">
        insert into user(id,name,pwd) values(#{id},#{name},#{pwd});
    </insert>
</mapper>